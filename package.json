    "laravel-mix": "^5.0.9"
  },
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "@tailwindcss/ui": "^0.7.2",
    "gh-pages": "^3.1.0",
    "tailwindcss": "^1.9.6"
  }
